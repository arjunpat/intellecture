{
  "compilerOptions": {
    "module": "commonjs",
    "esModuleInterop": true,
    "target": "es6",
    "moduleResolution": "node",
    "sourceMap": false,
    "outDir": "dist",
    "allowJs": true,
    "inlineSourceMap": false,
    "checkJs": true,
    "strict": true,
    "noImplicitAny": false,
    "noImplicitReturns": false,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "strictFunctionTypes": false
  },
  "files": ["server.ts"],
  "lib": ["es2015"]
}
{
  "name": "teacher-hud-electron",
  "productName": "teacher-hud-electron",
  "version": "1.0.0",
  "description": "Teacher HUD for Intellecture",
  "main": "src/main.js",
  "scripts": {
    "start": "electron .",
    "lint": "echo \"No linting configured\"",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "build": {
    "productName": "IntellectureTeacherHud",
    "appId": "app.intellecture.teacher-hud",
    "files": [
      "node_modules/",
      "src/public/",
      "src/main.js",
      "package.json"
    ],
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "win": {
      "target": [
        "squirrel"
      ],
      "icon": "build/icon.ico"
    },
    "linux": {
      "target": [
        "deb",
        "rpm",
        "AppImage"
      ],
      "category": "Development"
    }
  },
  "keywords": [],
  "author": {
    "name": "Intellecture Team",
    "email": "contact@intellecture.app"
  },
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "electron": "9.2.0",
    "electron-builder": "^22.8.0"
  }
}
